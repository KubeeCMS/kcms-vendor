{
    "name": "kubeecms/kcms-vendor",
    "type": "wordpress-plugin",
    "license": "GNU",
    "description": "E-commerce marketplace",
    "homepage": "https://github.com/KubeeCMS/kcms-vendor/",
    "keywords": ["kubeecms", "kubee", "kcms-ul"],
    "support": {
        "issues": "https://github.com/KubeeCMS/kcms-vendor/issues/",
        "forum": "https://discourse.kubee.com/"
    },
    "require": {
        "php": ">=7.4",
        "appsero/client": "dev-master",
        "jakeasmith/http_build_url": "^1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "wp-coding-standards/wpcs": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "dev-master",
        "tareq1988/wp-php-cs-fixer": "dev-master",
        "phpcompatibility/phpcompatibility-wp": "dev-master",
        "phpunit/phpunit": ">=5.7.27"
    },
    "config": {
        "platform": {
          "php": "8"
        }
      },
      "autoload": {
        "psr-4": {
          "WeDevs\\Dokan\\": "includes/"
        }
      },
      "scripts": {
        "phpcs": [
          "phpcs -p -s"
        ],
        "phpcbf": [
          "phpcbf -p"
        ]
      }
}